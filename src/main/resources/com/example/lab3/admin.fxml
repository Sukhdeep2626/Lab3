<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>

<?import javafx.scene.control.Label?>
<VBox alignment="CENTER" prefHeight="400.0" prefWidth="400.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab3.adminController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <TextField fx:id="iid" promptText="ID" />
    <TextField fx:id="iname" promptText="Name"/>
    <TextField fx:id="iposition" promptText="Position" />
    <TextField fx:id="isalary" promptText="Salary" />


    <Button mnemonicParsing="false" onAction="#InsertData" text=" Insert" />
    <Button mnemonicParsing="false" onAction="#UpdateData" text="Admin - Update" />
    <Button mnemonicParsing="false" onAction="#DeleteData" text="Admin - Delete" />
    <Button mnemonicParsing="false" onAction="#loadData" text="Admin - Load" />
    <Label fx:id="demo"/>
    <Button onAction="#onHelloButtonClick" text="Fetch" />
    <Button text="Exit" onAction="#logoutClick" />






    <TableView fx:id="tableView" prefHeight="200.0" prefWidth="300.0">
        <columns>
            <TableColumn fx:id="id" text="ID" />
            <TableColumn fx:id="name" text="Name" />
            <TableColumn fx:id="position" text="Position" />
            <TableColumn fx:id="salary" text="Salary" />


        </columns>
    </TableView>
</VBox>



